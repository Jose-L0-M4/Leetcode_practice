class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        a = []
        for i in range(len(nums)):
            b = target - nums[i]
            if b in nums and nums.index(b) != i and i not in a and nums.index(b) not in a:
                a.append(i)
                
                a.append(nums.index(b))
            
                



        return a

        
