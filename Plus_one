class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        value = ""
        for i in digits:
            value+= str(i)
        value = int(value)
        value+=1
        list2 = [x for x in str(value)]
        for i in range(len(list2)):
            list2[i]= int(list2[i])
        return list2
        

        
